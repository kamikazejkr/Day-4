#include <stdio.h>
int p(int n);
int main()
{
    int n;
    printf("\nHow many numbers do u want to enter?\n");
    scanf("%d",&n);
    p(n);
    return 0;
}
int p(int n)
{
    int i,s[100];
    printf("\nEnter the numbers to be added\n");
    for(i=0;i<n;i++)
      scanf("%d",&s[i]);
    printf("\nThe entered numbers are: \n");
    for(i=0;i<n;i++)
      printf("%d\n",s[i]);
    return 0;
}
